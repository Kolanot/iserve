@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix msm: <http://iserve.kmi.open.ac.uk/ns/msm#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://iserve.kmi.open.ac.uk/ns/msm#>
    dc:created "2010-03-10"^^xsd:date ;
    dc:description "This is a simple ontology that provides the core vocabulary for capturing service properties" ;
    dc:modified "2011-12-03"^^xsd:date, "2012-03-14"^^xsd:date, "2013-05-03"^^xsd:date ;
    a owl:Ontology ;
    rdfs:label "Minimal Service Model" ;
    owl:imports <http://www.w3.org/ns/sawsdl>, <http://www.wsmo.org/ns/wsmo-lite> ;
    owl:versionInfo "1.2" .

msm:MessageContent
    a rdfs:Class ;
    rdfs:label "MessageContent" ;
    rdfs:subClassOf msm:MessagePart .

msm:MessagePart
    a rdfs:Class ;
    rdfs:label "MessagePart" .

msm:Operation
    a rdfs:Class ;
    rdfs:label "Operation" .

msm:Service
    a rdfs:Class ;
    rdfs:label "Service" .

msm:hasInput
    a rdf:Property ;
    rdfs:domain msm:Operation ;
    rdfs:label "hasInput" ;
    rdfs:range msm:MessageContent .

msm:hasInputFault
    a rdf:Property ;
    rdfs:domain msm:Operation ;
    rdfs:label "hasInputFault" ;
    rdfs:range msm:MessageContent .

msm:hasMandatoryPart
    a rdf:Property ;
    rdfs:label "hasMandatoryPart" ;
    rdfs:subPropertyOf msm:hasPart .

msm:hasName
    a rdf:Property ;
    rdfs:domain msm:MessagePart ;
    rdfs:label "hasName" ;
    rdfs:range rdfs:Literal .

msm:hasOperation
    a rdf:Property ;
    rdfs:domain msm:Service ;
    rdfs:label "hasOperation" ;
    rdfs:range msm:Operation .

msm:hasOptionalPart
    a rdf:Property ;
    rdfs:label "hasOptionalPart" ;
    rdfs:subPropertyOf msm:hasPart .

msm:hasOutput
    a rdf:Property ;
    rdfs:domain msm:Operation ;
    rdfs:label "hasOutput" ;
    rdfs:range msm:MessageContent .

msm:hasOutputFault
    a rdf:Property ;
    rdfs:domain msm:Operation ;
    rdfs:label "hasOutputFault" ;
    rdfs:range msm:MessageContent .

msm:hasPart
    a rdf:Property ;
    rdfs:label "hasPart" ;
    rdfs:subPropertyOf msm:hasPartTransitive .

msm:hasPartTransitive
    a rdf:Property, owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:domain msm:MessagePart ;
    rdfs:label "hasPartTransitive" ;
    rdfs:range msm:MessagePart .

