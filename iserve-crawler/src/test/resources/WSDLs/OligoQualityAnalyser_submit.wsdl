<?xml version="1.0"?>
<wsdl:definitions name="MOBY_Central_Generated_WSDL"
                targetNamespace="http://biomoby.org/Central.wsdl"
                xmlns:tns="http://biomoby.org/Central.wsdl"
                xmlns:xsd1="http://biomoby.org/CentralXSDs.xsd"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/">

                 
  <wsdl:message name="OligoQualityAnalyser_submitInput">
          <wsdl:part name="data" type="xsd:string"/>
  </wsdl:message>

  <wsdl:message name="OligoQualityAnalyser_submitOutput">
          <wsdl:part name="body" type="xsd:string"/>
  </wsdl:message>

  <wsdl:portType name="OligoQualityAnalyser_submitPortType">
          <wsdl:operation name="OligoQualityAnalyser_submit">
                 <wsdl:input message="tns:OligoQualityAnalyser_submitInput"/>
                 <wsdl:output message="tns:OligoQualityAnalyser_submitOutput"/>
          </wsdl:operation>
  </wsdl:portType>
                
  <wsdl:binding name="OligoQualityAnalyser_submitBinding" type="tns:OligoQualityAnalyser_submitPortType">
          <wsdlsoap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
          <wsdl:operation name="OligoQualityAnalyser_submit"><!-- in essense, this is the name of the subroutine that is called -->
                 <wsdlsoap:operation soapAction='http://biomoby.org/#OligoQualityAnalyser_submit' style='rpc'/>
                 <wsdl:input>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:input>
                 <wsdl:output>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:output>
          </wsdl:operation>
  </wsdl:binding>
                
  <wsdl:service name="OligoQualityAnalyser_submitService">
          <wsdl:documentation>Authority: www.bioinformatics.nl  -  Asynchronous BioMOBY web service that takes a collection of OligoAnnotationAnalyser service outputs as input and creates:
 * Array quality summaries (Collection of URLs pointing to Collections of GraphSeries1D objects) and 
 * Oligo quality per chunk (Collection of URLs pointing to chunks of Collections of OligoQualityRecord objects). 
	* Oligo quality is determined by calculating whether a hit is capable of generating signal in an array expermiment 
	  based on criteria specified in an OligoQualityClassThresholds object.</wsdl:documentation>  <!-- service description goes here -->
          <wsdl:port name="OligoQualityAnalyser_submitPort" binding="tns:OligoQualityAnalyser_submitBinding">
                 <wsdlsoap:address location="https://www.bioinformatics.nl/phenolink/biomoby/services/cgi-bin/LocalServices.cgi"/>    <!-- URL to service scriptname -->
          </wsdl:port>
  </wsdl:service>

</wsdl:definitions>


