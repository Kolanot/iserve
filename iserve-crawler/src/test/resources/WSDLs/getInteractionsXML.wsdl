<?xml version="1.0"?>
<wsdl:definitions name="MOBY_Central_Generated_WSDL"
                targetNamespace="http://biomoby.org/Central.wsdl"
                xmlns:tns="http://biomoby.org/Central.wsdl"
                xmlns:xsd1="http://biomoby.org/CentralXSDs.xsd"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/">

                 
  <wsdl:message name="getInteractionsXMLInput">
          <wsdl:part name="data" type="xsd:string"/>
  </wsdl:message>

  <wsdl:message name="getInteractionsXMLOutput">
          <wsdl:part name="body" type="xsd:string"/>
  </wsdl:message>

  <wsdl:portType name="getInteractionsXMLPortType">
          <wsdl:operation name="getInteractionsXML">
                 <wsdl:input message="tns:getInteractionsXMLInput"/>
                 <wsdl:output message="tns:getInteractionsXMLOutput"/>
          </wsdl:operation>
  </wsdl:portType>
                
  <wsdl:binding name="getInteractionsXMLBinding" type="tns:getInteractionsXMLPortType">
          <wsdlsoap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
          <wsdl:operation name="getInteractionsXML"><!-- in essense, this is the name of the subroutine that is called -->
                 <wsdlsoap:operation soapAction='http://biomoby.org/#getInteractionsXML' style='rpc'/>
                 <wsdl:input>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:input>
                 <wsdl:output>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:output>
          </wsdl:operation>
  </wsdl:binding>
                
  <wsdl:service name="getInteractionsXMLService">
          <wsdl:documentation>Authority: pdg.cnb.uam.es  -  It returns a list with the different IntAct_XML interactions where a protein IDs is interacting</wsdl:documentation>  <!-- service description goes here -->
          <wsdl:port name="getInteractionsXMLPort" binding="tns:getInteractionsXMLBinding">
                 <wsdlsoap:address location="http://www.pdg.cnb.uam.es/moby/cgi-bin/mobyservice"/>    <!-- URL to service scriptname -->
          </wsdl:port>
  </wsdl:service>

</wsdl:definitions>


