<?xml version="1.0"?>
<wsdl:definitions name="MOBY_Central_Generated_WSDL"
                targetNamespace="http://biomoby.org/Central.wsdl"
                xmlns:tns="http://biomoby.org/Central.wsdl"
                xmlns:xsd1="http://biomoby.org/CentralXSDs.xsd"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/">

                 
  <wsdl:message name="getPDBLigandStructureInput">
          <wsdl:part name="data" type="xsd:string"/>
  </wsdl:message>

  <wsdl:message name="getPDBLigandStructureOutput">
          <wsdl:part name="body" type="xsd:string"/>
  </wsdl:message>

  <wsdl:portType name="getPDBLigandStructurePortType">
          <wsdl:operation name="getPDBLigandStructure">
                 <wsdl:input message="tns:getPDBLigandStructureInput"/>
                 <wsdl:output message="tns:getPDBLigandStructureOutput"/>
          </wsdl:operation>
  </wsdl:portType>
                
  <wsdl:binding name="getPDBLigandStructureBinding" type="tns:getPDBLigandStructurePortType">
          <wsdlsoap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
          <wsdl:operation name="getPDBLigandStructure"><!-- in essense, this is the name of the subroutine that is called -->
                 <wsdlsoap:operation soapAction='http://biomoby.org/#getPDBLigandStructure' style='rpc'/>
                 <wsdl:input>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:input>
                 <wsdl:output>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:output>
          </wsdl:operation>
  </wsdl:binding>
                
  <wsdl:service name="getPDBLigandStructureService">
          <wsdl:documentation>Authority: mmb.pcb.ub.es  -  
		Given a PDB code and a PDB Ligand code, returns the PDB-text of the ligand into this PDB structure
	</wsdl:documentation>  <!-- service description goes here -->
          <wsdl:port name="getPDBLigandStructurePort" binding="tns:getPDBLigandStructureBinding">
                 <wsdlsoap:address location="http://mmb2.pcb.ub.es:8080/PDBServices/getPDBLigandStructure"/>    <!-- URL to service scriptname -->
          </wsdl:port>
  </wsdl:service>

</wsdl:definitions>


