<?xml version="1.0"?>
<wsdl:definitions name="MOBY_Central_Generated_WSDL"
                targetNamespace="http://biomoby.org/Central.wsdl"
                xmlns:tns="http://biomoby.org/Central.wsdl"
                xmlns:xsd1="http://biomoby.org/CentralXSDs.xsd"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/">

                 
  <wsdl:message name="runCloverGFFCollectionInput">
          <wsdl:part name="data" type="xsd:string"/>
  </wsdl:message>

  <wsdl:message name="runCloverGFFCollectionOutput">
          <wsdl:part name="body" type="xsd:string"/>
  </wsdl:message>

  <wsdl:portType name="runCloverGFFCollectionPortType">
          <wsdl:operation name="runCloverGFFCollection">
                 <wsdl:input message="tns:runCloverGFFCollectionInput"/>
                 <wsdl:output message="tns:runCloverGFFCollectionOutput"/>
          </wsdl:operation>
  </wsdl:portType>
                
  <wsdl:binding name="runCloverGFFCollectionBinding" type="tns:runCloverGFFCollectionPortType">
          <wsdlsoap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
          <wsdl:operation name="runCloverGFFCollection"><!-- in essense, this is the name of the subroutine that is called -->
                 <wsdlsoap:operation soapAction='http://biomoby.org/#runCloverGFFCollection' style='rpc'/>
                 <wsdl:input>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:input>
                 <wsdl:output>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:output>
          </wsdl:operation>
  </wsdl:binding>
                
  <wsdl:service name="runCloverGFFCollectionService">
          <wsdl:documentation>Authority: genome.imim.es  -  Uses Clover to analyze a collection of DNA sequences for putative transcription factor binding sites from Transfac or Jaspar and reports them in GFF format</wsdl:documentation>  <!-- service description goes here -->
          <wsdl:port name="runCloverGFFCollectionPort" binding="tns:runCloverGFFCollectionBinding">
                 <wsdlsoap:address location="http://genome.imim.es/cgi-bin/moby/devel/MobyServices.cgi"/>    <!-- URL to service scriptname -->
          </wsdl:port>
  </wsdl:service>

</wsdl:definitions>


