<?xml version="1.0"?>
<wsdl:definitions name="MOBY_Central_Generated_WSDL"
                targetNamespace="http://biomoby.org/Central.wsdl"
                xmlns:tns="http://biomoby.org/Central.wsdl"
                xmlns:xsd1="http://biomoby.org/CentralXSDs.xsd"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/">

                 
  <wsdl:message name="runPreprocessingInput">
          <wsdl:part name="data" type="xsd:string"/>
  </wsdl:message>

  <wsdl:message name="runPreprocessingOutput">
          <wsdl:part name="body" type="xsd:string"/>
  </wsdl:message>

  <wsdl:portType name="runPreprocessingPortType">
          <wsdl:operation name="runPreprocessing">
                 <wsdl:input message="tns:runPreprocessingInput"/>
                 <wsdl:output message="tns:runPreprocessingOutput"/>
          </wsdl:operation>
  </wsdl:portType>
                
  <wsdl:binding name="runPreprocessingBinding" type="tns:runPreprocessingPortType">
          <wsdlsoap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
          <wsdl:operation name="runPreprocessing"><!-- in essense, this is the name of the subroutine that is called -->
                 <wsdlsoap:operation soapAction='http://biomoby.org/#runPreprocessing' style='rpc'/>
                 <wsdl:input>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:input>
                 <wsdl:output>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:output>
          </wsdl:operation>
  </wsdl:binding>
                
  <wsdl:service name="runPreprocessingService">
          <wsdl:documentation>Authority: bioinfo.cipf.es  -  Preprocessing service for gene expression patterns that allows users to apply log-transform, merge replicated values, filter missing values and impute missing values</wsdl:documentation>  <!-- service description goes here -->
          <wsdl:port name="runPreprocessingPort" binding="tns:runPreprocessingBinding">
                 <wsdlsoap:address location="http://biomoby.bioinfo.cipf.es/cgi-bin/MobyServices.cgi"/>    <!-- URL to service scriptname -->
          </wsdl:port>
  </wsdl:service>

</wsdl:definitions>


