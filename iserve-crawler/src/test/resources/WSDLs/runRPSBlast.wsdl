<?xml version="1.0"?>
<wsdl:definitions name="MOBY_Central_Generated_WSDL"
                targetNamespace="http://biomoby.org/Central.wsdl"
                xmlns:tns="http://biomoby.org/Central.wsdl"
                xmlns:xsd1="http://biomoby.org/CentralXSDs.xsd"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/">

                 
  <wsdl:message name="runRPSBlastInput">
          <wsdl:part name="data" type="xsd:string"/>
  </wsdl:message>

  <wsdl:message name="runRPSBlastOutput">
          <wsdl:part name="body" type="xsd:string"/>
  </wsdl:message>

  <wsdl:portType name="runRPSBlastPortType">
          <wsdl:operation name="runRPSBlast">
                 <wsdl:input message="tns:runRPSBlastInput"/>
                 <wsdl:output message="tns:runRPSBlastOutput"/>
          </wsdl:operation>
  </wsdl:portType>
                
  <wsdl:binding name="runRPSBlastBinding" type="tns:runRPSBlastPortType">
          <wsdlsoap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
          <wsdl:operation name="runRPSBlast"><!-- in essense, this is the name of the subroutine that is called -->
                 <wsdlsoap:operation soapAction='http://biomoby.org/#runRPSBlast' style='rpc'/>
                 <wsdl:input>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:input>
                 <wsdl:output>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:output>
          </wsdl:operation>
  </wsdl:binding>
                
  <wsdl:service name="runRPSBlastService">
          <wsdl:documentation>Authority: inb.bsc.es  -  RPS-BLAST (Reverse PSI-BLAST) searches a query sequence against a database of profiles. </wsdl:documentation>  <!-- service description goes here -->
          <wsdl:port name="runRPSBlastPort" binding="tns:runRPSBlastBinding">
                 <wsdlsoap:address location="http://inb.bsc.es/cgi-bin/mobyServices/dispatchers/asyncDispatcher.cgi"/>    <!-- URL to service scriptname -->
          </wsdl:port>
  </wsdl:service>

</wsdl:definitions>


