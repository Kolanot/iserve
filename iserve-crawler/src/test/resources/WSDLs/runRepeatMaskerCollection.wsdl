<?xml version="1.0"?>
<wsdl:definitions name="MOBY_Central_Generated_WSDL"
                targetNamespace="http://biomoby.org/Central.wsdl"
                xmlns:tns="http://biomoby.org/Central.wsdl"
                xmlns:xsd1="http://biomoby.org/CentralXSDs.xsd"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/">

                 
  <wsdl:message name="runRepeatMaskerCollectionInput">
          <wsdl:part name="data" type="xsd:string"/>
  </wsdl:message>

  <wsdl:message name="runRepeatMaskerCollectionOutput">
          <wsdl:part name="body" type="xsd:string"/>
  </wsdl:message>

  <wsdl:portType name="runRepeatMaskerCollectionPortType">
          <wsdl:operation name="runRepeatMaskerCollection">
                 <wsdl:input message="tns:runRepeatMaskerCollectionInput"/>
                 <wsdl:output message="tns:runRepeatMaskerCollectionOutput"/>
          </wsdl:operation>
  </wsdl:portType>
                
  <wsdl:binding name="runRepeatMaskerCollectionBinding" type="tns:runRepeatMaskerCollectionPortType">
          <wsdlsoap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
          <wsdl:operation name="runRepeatMaskerCollection"><!-- in essense, this is the name of the subroutine that is called -->
                 <wsdlsoap:operation soapAction='http://biomoby.org/#runRepeatMaskerCollection' style='rpc'/>
                 <wsdl:input>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:input>
                 <wsdl:output>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:output>
          </wsdl:operation>
  </wsdl:binding>
                
  <wsdl:service name="runRepeatMaskerCollectionService">
          <wsdl:documentation>Authority: genome.imim.es  -  Runs RepeatMasker to mask repeat sequences a set of DNA sequences</wsdl:documentation>  <!-- service description goes here -->
          <wsdl:port name="runRepeatMaskerCollectionPort" binding="tns:runRepeatMaskerCollectionBinding">
                 <wsdlsoap:address location="http://genome.imim.es/cgi-bin/moby/MobyServices.cgi"/>    <!-- URL to service scriptname -->
          </wsdl:port>
  </wsdl:service>

</wsdl:definitions>


