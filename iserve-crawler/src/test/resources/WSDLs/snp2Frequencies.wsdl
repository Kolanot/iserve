<?xml version="1.0"?>
<wsdl:definitions name="MOBY_Central_Generated_WSDL"
                targetNamespace="http://biomoby.org/Central.wsdl"
                xmlns:tns="http://biomoby.org/Central.wsdl"
                xmlns:xsd1="http://biomoby.org/CentralXSDs.xsd"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                xmlns="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/">

                 
  <wsdl:message name="snp2FrequenciesInput">
          <wsdl:part name="data" type="xsd:string"/>
  </wsdl:message>

  <wsdl:message name="snp2FrequenciesOutput">
          <wsdl:part name="body" type="xsd:string"/>
  </wsdl:message>

  <wsdl:portType name="snp2FrequenciesPortType">
          <wsdl:operation name="snp2Frequencies">
                 <wsdl:input message="tns:snp2FrequenciesInput"/>
                 <wsdl:output message="tns:snp2FrequenciesOutput"/>
          </wsdl:operation>
  </wsdl:portType>
                
  <wsdl:binding name="snp2FrequenciesBinding" type="tns:snp2FrequenciesPortType">
          <wsdlsoap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
          <wsdl:operation name="snp2Frequencies"><!-- in essense, this is the name of the subroutine that is called -->
                 <wsdlsoap:operation soapAction='http://biomoby.org/#snp2Frequencies' style='rpc'/>
                 <wsdl:input>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:input>
                 <wsdl:output>
                         <wsdlsoap:body use="encoded" namespace="http://biomoby.org/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
                 </wsdl:output>
          </wsdl:operation>
  </wsdl:binding>
                
  <wsdl:service name="snp2FrequenciesService">
          <wsdl:documentation>Authority: bioinfo.icapture.ubc.ca  -  Retrieves a list of genotype and allele frequencies for non-synonymous SNPs genotyped by 
HapMap (www.hapmap.org) from the snp input identifier.</wsdl:documentation>  <!-- service description goes here -->
          <wsdl:port name="snp2FrequenciesPort" binding="tns:snp2FrequenciesBinding">
                 <wsdlsoap:address location="http://dev.biordf.net/mobyservices/services/snp2Frequencies"/>    <!-- URL to service scriptname -->
          </wsdl:port>
  </wsdl:service>

</wsdl:definitions>


