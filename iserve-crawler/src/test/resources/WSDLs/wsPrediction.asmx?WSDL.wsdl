  <wsdl:portType name="wsPredictionSoap">
    <wsdl:operation name="GetMiningModels">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">This service returns a list of all available MiningModels for the classification/prediction 
                            with appropiate information regarding training set size and cross validation performance.
                            This service is used to populate the GridView at the &lt;a href="/decisiontree.aspx"&gt;decisiontree.aspx&lt;/a&gt; page.</wsdl:documentation>
      <wsdl:input message="tns:GetMiningModelsSoapIn" />
      <wsdl:output message="tns:GetMiningModelsSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetMiningModelNodes">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">This service returns all nodes constituting the decision tree used in a particular mining model.
                            &lt;strong&gt;id&lt;/strong&gt; reference the mining model.</wsdl:documentation>
      <wsdl:input message="tns:GetMiningModelNodesSoapIn" />
      <wsdl:output message="tns:GetMiningModelNodesSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="PredictAll4AlkaneRiColumnTypeComposition">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Predicts the presense of a set of functional groups by the means of
                            an retention index &lt;strong&gt;ri&lt;/strong&gt;, an GC-EI mass spectrum &lt;strong&gt;spectrum&lt;/strong&gt; and an 
                            ri system &lt;strong&gt;ColumnType&lt;/strong&gt; ('none' or 'VAR5' or 'MDN35').</wsdl:documentation>
      <wsdl:input message="tns:PredictAll4AlkaneRiColumnTypeCompositionSoapIn" />
      <wsdl:output message="tns:PredictAll4AlkaneRiColumnTypeCompositionSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="PredictSingle">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Predicts the presense of a set of functional groups by the means of
                            an retention index based on the alkanes homolouges &lt;strong&gt;ri&lt;/strong&gt; and 
                            an GC-EI mass spectrum &lt;strong&gt;spectrum&lt;/strong&gt; and an particular Mining Model (Decision Tree) &lt;strong&gt;MiningModelId&lt;/strong&gt;.</wsdl:documentation>
      <wsdl:input message="tns:PredictSingleSoapIn" />
      <wsdl:output message="tns:PredictSingleSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="PredictMany">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Predicts the presense of a set of functional groups by the means of
                            an retention index based on the alkanes homolouges &lt;strong&gt;ri&lt;/strong&gt; and 
                            an GC-EI mass spectrum &lt;strong&gt;spectrum&lt;/strong&gt; and an collection (Array) of particular Mining Models (Decision Trees) &lt;strong&gt;MiningModelIds&lt;/strong&gt;.</wsdl:documentation>
      <wsdl:input message="tns:PredictManySoapIn" />
      <wsdl:output message="tns:PredictManySoapOut" />
    </wsdl:operation>
    <wsdl:operation name="DrillThrough">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Returns all training spectra IDs passing an particular &lt;strong&gt;NodeID&lt;/strong&gt; (i.e. '000000000020001') of a given Mining Model
                                    identified by the name &lt;strong&gt;MiningModelName&lt;/strong&gt; (cf. &lt;a href="/decisiontree.aspx"&gt;decisiontree.aspx&lt;/a&gt;, i.e.'2009-02-03T165023_Fg_Alpha_Aminoacid_MDN35').</wsdl:documentation>
      <wsdl:input message="tns:DrillThroughSoapIn" />
      <wsdl:output message="tns:DrillThroughSoapOut" />
    </wsdl:operation>
  </wsdl:portType>
