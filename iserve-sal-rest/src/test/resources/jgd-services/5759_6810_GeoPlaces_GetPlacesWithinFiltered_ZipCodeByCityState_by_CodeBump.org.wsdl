    <wsdl:port name="GeoPlacesHttpPost" binding="tns:GeoPlacesHttpPost">
      <http:address location="http://codebump.com/services/placelookup.asmx" />
    </wsdl:port>
  </wsdl:service>
  
</wsdl:definitions>
