      <soap12:address location="http://codebump.com/services/placelookup.asmx" />
    </wsdl:port>
    <wsdl:port name="GeoPlacesHttpGet" binding="tns:GeoPlacesHttpGet">
      <http:address location="http://codebump.com/services/placelookup.asmx" />
